{
  "name": "jxpush",
  "version": "1.0.8",
  "description": "Production-grade unified push notification library for Node.js",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist",
    "bin",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "setup": "node dist/cjs/cli/setup.js",
    "build": "yarn build:clean && yarn build:esm && yarn build:cjs && yarn build:types",
    "build:clean": "rm -rf dist",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json && echo '{\"type\": \"commonjs\"}' > dist/cjs/package.json",
    "build:types": "tsc -p tsconfig.types.json",
    "test": "jest --testPathIgnorePatterns=integration",
    "test:unit": "jest --testPathIgnorePatterns=integration",
    "test:integration": "jest --testPathPattern=integration",
    "test:all": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\" \"examples/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\" \"examples/**/*.ts\"",
    "prepublishOnly": "yarn build && yarn test && yarn lint"
  },
  "keywords": [
    "push",
    "notification",
    "fcm",
    "firebase",
    "expo",
    "webpush",
    "vapid",
    "email",
    "resend",
    "smtp",
    "kafka",
    "rabbitmq",
    "messaging",
    "push-notifications",
    "mobile",
    "queue",
    "redis",
    "bullmq",
    "rate-limit",
    "retry",
    "bulk-send",
    "template",
    "localization",
    "cli",
    "modular",
    "zero-bloat",
    "plugin"
  ],
  "author": "jxngrx",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jxtools/jxpush.git"
  },
  "bugs": {
    "url": "https://github.com/jxtools/jxpush/issues"
  },
  "homepage": "https://jxpush.jxngrx.com",
  "engines": {
    "node": ">=20.0.0"
  },
  "bin": {
    "jxpush": "./bin/jxpush.cjs"
  },
  "dependencies": {
    "chalk": "^5.6.2",
    "cli-table3": "^0.6.5",
    "commander": "^11.0.0",
    "inquirer": "^9.2.23"
  },
  "peerDependencies": {
    "amqplib": "^0.10.0",
    "bullmq": "^5.0.0",
    "expo-server-sdk": "^5.0.0",
    "firebase-admin": "^12.0.0",
    "handlebars": "^4.7.0",
    "ioredis": "^5.0.0",

    "nodemailer": "^8.0.0",
    "resend": "^6.0.0",
    "web-push": "^3.6.0"
  },
  "peerDependenciesMeta": {
    "firebase-admin": {
      "optional": true
    },
    "expo-server-sdk": {
      "optional": true
    },
    "bullmq": {
      "optional": true
    },

    "amqplib": {
      "optional": true
    },
    "ioredis": {
      "optional": true
    },
    "resend": {
      "optional": true
    },
    "nodemailer": {
      "optional": true
    },
    "web-push": {
      "optional": true
    },
    "handlebars": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.8",
    "@types/express": "^5.0.6",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.19.33",
    "@types/nodemailer": "^7.0.9",
    "@types/web-push": "^3.6.4",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "amqplib": "^0.10.9",
    "bullmq": "^5.69.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "expo-server-sdk": "^5.0.0",
    "firebase-admin": "^12.0.0",
    "handlebars": "^4.7.8",
    "ioredis": "^5.9.3",
    "jest": "^29.7.0",

    "nodemailer": "^8.0.1",
    "prettier": "^3.2.5",
    "resend": "^6.9.2",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"

  }
}
